package world;

import entity.Entity;

import java.util.ArrayList;
import java.util.List;

public class World {
	private ArrayList<Entity> Entities;
	
	public World (){}
	
	public void addEntity(Entity newEntity){
		Entities.add(newEntity);
	}
	
	public ArrayList<Entity> getEntitys(){
		return Entities;
	}
}
